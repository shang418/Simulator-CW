package Avatars;

import javax.swing.JLabel;

public class PlayerUAV extends JLabel {
	private double _x,_y,_theta;
	static final int WIDTH = 1400;
	static final int HEIGHT = 800;
	double _startPose = {WIDTH - 50,_HEIGHT - 50};
	
	

	public PlayerUAV(){
			
	}
	
	private double getXPos(){
		return _x;
	}
	
	
	private void updatePose(){
		_x = _startPose;
		_y= _startPose;
	}
	

}
